{
  "name": "vue-shum-ui",
  "version": "1.0.0",
  "description": "Библиотека UI компонентов для Vue 3 + TypeScript",
  "author": "shumaf",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shumafbatov/vue-ui-kit.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "typescript",
    "ui",
    "components",
    "library",
    "vue-components"
  ],
  "type": "module",
  "main": "./dist/vue-shum-ui.umd.js",
  "module": "./dist/vue-shum-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-shum-ui.es.js",
      "require": "./dist/vue-shum-ui.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "vue-tsc --noEmit && vite build --mode library",
    "build:demo": "vite build --mode demo",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:ui:alt": "cross-env VITEST_UI_PORT=51206 vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "prepublishOnly": "npm run build:lib",
    "publish:patch": "npm run version:patch && npm publish --access public",
    "publish:minor": "npm run version:minor && npm publish --access public",
    "publish:major": "npm run version:major && npm publish --access public"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  }
}
